<?php

namespace FarmBundle\Entity;

/**
 * Farm_I
 */
class Farm_I
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var int
     */
    private $region;

    /**
     * @var string
     */
    private $name;

    /**
     * @var string
     */
    private $image;

    /**
     * @var string
     */
    private $description;

    /**
     * @var string
     */
    private $elevation;

    /**
     * @var string
     */
    private $harvest;

    /**
     * @var float
     */
    private $latitude;

    /**
     * @var float
     */
    private $longitude;
  
    /**
     * @var ArrayCollection
     */
    private $products;
  
    /**
     * @var ArrayCollection
     */
    private $certifications;
   
    /**
     * @var ArrayCollection
     */
    private $farm_awards;
  
    /**
     * @var ArrayCollection
     */
    private $species;
  
    /**
     * @var ArrayCollection
     */
    private $cultivars;
}
